pico-8 cartridge // http://www.pico-8.com
version 7
__lua__
-- multidimensional arrays
a = {{1,2,3}, {4,5,6}, {7,8,9}}

-- print part of array
print (a[1][1])

-- print at screen location
print (a[1][1],10,10)
-- return cursor
cursor (0,20)

-- print all of array
for y = 1, 3 do
    for x = 1, 3 do
         print(a[y][x])
    end
end

-- determine size
print (#a)

-- for additional information see - http://blog.headchant.com/arrays-in-pico-8/
